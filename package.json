    ],
    "related": {
      "list": [
        "generate",
        "generate-file",
        "generate-node"
      ]
    },
    "reflinks": [
      "assemble",
      "generate",
      "gulp",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
