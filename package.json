{
  "name": "generate-dest",
  "description": "`Generate` generator that prompts the user for the destination directory to use. Can be used as a sub-generator or plugin in your generator.",
  "version": "0.1.2",
  "homepage": "https://github.com/generate/generate-dest",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-dest",
  "bugs": {
    "url": "https://github.com/generate/generate-dest/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.1.0"
  },
  "devDependencies": {
    "generate": "^0.7.1",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "contents",
    "create",
    "file",
    "fs",
    "generate",
    "generator",
    "init",
